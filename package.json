    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "terser": "^4.8.1"
  },
  "lint-staged": {
